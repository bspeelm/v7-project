  "trainingPrinciples": {
    "progressive_overload": {
      "definition": "Gradually increasing training stimulus to force adaptation",
      "application": "Increase difficulty through grade, volume, intensity, or density",
      "guidelines": "No more than 10-20% increase per week in total load"
    },
    "specificity": {
      "definition": "Training should match the demands of your goal",
      "application": "V7 requires power-endurance, finger strength, and complex movement",
      "guidelines": "70% of training should mimic goal-route style"
    },
    "recovery": {
      "definition": "Adaptation occurs during rest, not training",
      "application": "48-72 hours between hard sessions targeting same system",
      "guidelines": "1-2 full rest days per week, deload every 4-6 weeks"
    },
    "periodization": {
      "definition": "Systematic variation of training variables over time",
      "phases": {
